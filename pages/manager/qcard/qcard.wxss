page{
  padding-top:20rpx;
  padding-bottom:100rpx;
}

.btnsection{
  position:fixed;
  bottom:0;
  left:0;
  display:flex;
  justify-content: space-between;
  align-items: center;
  width:690rpx;
  height:80rpx;
  padding:20rpx 30rpx;
  background-color:#fff;
  box-shadow: 0 -5rpx 10rpx #ccc;
}
.btnsection view{
  display:flex;
  justify-content: center;
  align-items: center;
  width:320rpx;
  height:80rpx;
  /* border:1px solid #3F89E8;
  border-radius: 12rpx; */
  color:#323232;
  font-weight: bold;
  font-size: 34rpx;
  line-height:80rpx;
  text-align: center;
}

.btnsection image{
  width:40rpx;
  height:40rpx;
  margin-right:16rpx;
}
.btnsection button{
  display:flex;
  justify-content: center;
  align-items: center;
  background-color:#fff;
}

.btn-box{
  position:fixed;
  bottom:200rpx;
  right:10rpx;
  display:flex;
  justify-content: space-around;
  align-items: center;
  flex-flow: column;
  width:80rpx;
  height:180rpx;
  padding:20rpx 10rpx;
  background-color: #fff;
  border:1px solid #eee;
  border-radius: 12rpx;
  box-shadow: 0 5rpx 10rpx #ccc;
}
.obtain{
  width:62rpx;
  height:62rpx;
}
.obtain image{
  width:100%;
  height:100%;
}

.share{
  width:80rpx;
  height:80rpx;
  padding:0;
  background-color:#fff;
}
.share image{
  width:60rpx;
  height:60rpx;
  margin-top:10rpx;
  margin-left:6rpx;
}