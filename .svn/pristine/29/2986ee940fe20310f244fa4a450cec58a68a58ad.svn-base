.box{
  position:relative;
  width:100%;
  height:100%;
}
.btn-box{
  position:absolute;
  bottom:200rpx;
  right:10rpx;
  width:80rpx;
  height:180rpx;
  padding:20rpx 10rpx;
  background-color: #fff;
  border:1px solid #eee;
  border-radius: 12rpx;
  box-shadow: 0 5rpx 10rpx #ccc;
}
.obtain{
  width:80rpx;
  height:80rpx;
  margin-bottom:20rpx;
}
.obtain image{
  width:100%;
  height:100%;
}

.share{
  width:80rpx;
  height:80rpx;
  padding:0;
  background-color:#fff;

}
.share image{
  width:60rpx;
  height:60rpx;
  margin-top:10rpx;
  margin-left:6rpx;
}